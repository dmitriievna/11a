summa= 0
for i in range (1, n + 1):
        summa += i
print("skaitļu summa no 1 līdz n ir:", summa)

#1uzd
saraksts = ["skaits"]
for i in range(5):
    skaitlis = int(input("Ievadiet skaitli: "))
    saraksts.pop(1)
    saraksts.pop(5)
print("atlikušais saraksts: ", saraksts)

