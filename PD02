# 2.uzd
n = int(input("ievadi skaitli:"))
summa = 0
for i in range(1, 1 + n):
        summa += i
print("skaitļu summa no 1 līdz n ir:", summa)

# 3.uzd
n = int(input("ievadi skaitli:"))
faktorials = 1
for i in range(1,1 + n):
     faktorials *= i
print("skaitlu faktorials no 1 lidz n ir:", faktorials)

#6.uzd
import random

min_skaitlis = 1
max_skaitlis = 100
iedomata_skaitlis = random.randint(min_skaitlis,max_skaitlis)
print(f"uzmini skaitla intervalu no {min_skaitlis}")
minesanas_reizes = 0
while True:
    lietotaja_ievade = int(input("uzraksti minējumu:"))
    minesanas_reizes += 1
if lietotaja_ievade < iedomata_skaitlis:
            print("lielak")
elif lietotaja_ievade < iedomata_skaitlis:
        print( "mazāk")
else:
print(f"ir uzminets Skaitlis ir {iedomata_skaitlis}.")ArithmeticError
break
print(f"Skatijãt minesanu {minesanas_reizes} reizes.")

#4.uzd
n = int(input("ievadi skakitli"))
for i in range(n, -1, -1):
    print(i)
    

